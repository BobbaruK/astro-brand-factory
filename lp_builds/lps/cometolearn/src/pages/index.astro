---
// import HomePage from "@/layouts/Classic/home.astro";
// import HomePage from "@/layouts/ClassicRemake/home.astro";
// import HomePage from "@/layouts/ProfessionalBlack/home.astro";
import HomePage from "@/layouts/ProfessionalBlack-v2/home.astro";
import { useBrandFactory } from "../../../../globals/composables/useBrandFactory";
import { type TradeStrategyHubSVGVariation } from "../../../../globals/types/Brands/logo";
import { type ComponentProps } from "../../../../globals/types/ComponentProps";
import type {
  LandingPageLangType,
  LandingPageType,
} from "../../../../globals/types/LandingPageType";
import "../assets/styles/index.scss";

const langTypeRegion: LandingPageLangType = "cysec";
const lpType: LandingPageType = "classic";

export interface CustomOptions {
  logoSquare: boolean;
}
const componentProps: ComponentProps<
  TradeStrategyHubSVGVariation,
  CustomOptions
> = {
  lang: "en",
  langType: langTypeRegion,
  dir: "ltr",
  lpName: "Cometolearn",
  brandObj: useBrandFactory({
    brand: "tradestrategyhub",
    region: langTypeRegion,
    whatsapp: false,
    lpType: lpType,
  }),
  logo: function () {
    return this.brandObj.images.logoSVG.default;
  },
  lpType: lpType,
  formValidation: "classic",
  designVersion: "v2",
  customOptions: {
    logoSquare: false,
  },
};
---

<HomePage componentProps={componentProps} />
