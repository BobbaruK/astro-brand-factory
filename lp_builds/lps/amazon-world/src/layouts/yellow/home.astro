---
import Yellow from "@/layouts/yellow/pageLayout.astro";
import Section1 from "@/sections/yellow/home/Section1.astro";
import Section2 from "@/sections/yellow/home/Section2.astro";
import Section3 from "@/sections/yellow/home/Section3.astro";
import Section4 from "@/sections/yellow/home/Section4.astro";
import { type ComponentProps } from "../../../../../globals/types/ComponentProps";

interface Props {
  componentProps: ComponentProps;
}

const { componentProps } = Astro.props;
---

<Yellow componentProps={componentProps}>
  <Section1 componentProps={componentProps} />
  <Section2 componentProps={componentProps} />
  <Section3 componentProps={componentProps} />
  <Section4 componentProps={componentProps} />
</Yellow>
