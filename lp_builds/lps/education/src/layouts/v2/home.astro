---
import { type ComponentProps } from "../../../../../globals/types/ComponentProps";
import V2 from "@/layouts/v2/pageLayout.astro";
import Section1 from "@/sections/v2/home/Section1.astro";
import Section2 from "@/sections/v2/home/Section2.astro";
import Section3 from "@/sections/v2/home/Section3.astro";

interface Props {
  componentProps: ComponentProps;
}

const { componentProps } = Astro.props;
---

<V2 componentProps={componentProps}>
  <Section1 componentProps={componentProps} />
  <Section2 componentProps={componentProps} />
  <Section3 componentProps={componentProps} />
</V2>
