---
import Oil from "@/layouts/Oil/pageLayout.astro";
import Section1 from "@/sections/Oil/home/Section1.astro";
import Section2 from "@/sections/Oil/home/Section2.astro";
import Section3 from "@/sections/Oil/home/Section3.astro";
import { type ComponentProps } from "../../../../../globals/types/ComponentProps";

interface Props {
  componentProps: ComponentProps;
}

const { componentProps } = Astro.props;
---

<Oil componentProps={componentProps}>
  <Section1 componentProps={componentProps} />
  <Section2 componentProps={componentProps} />
  <Section3 componentProps={componentProps} />
</Oil>
