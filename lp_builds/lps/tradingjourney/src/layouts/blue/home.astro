---
import { type ComponentProps } from "../../../../../globals/types/ComponentProps";
import BlueLayout from "@/layouts/blue/pageLayout.astro";
import Section1 from "@/sections/blue/home/Section1.astro";
import Section2 from "@/sections/blue/home/Section2.astro";

interface Props {
  componentProps: ComponentProps;
}

const { componentProps } = Astro.props;
---

<BlueLayout componentProps={componentProps}>
  <Section1 componentProps={componentProps} />
  <Section2 componentProps={componentProps} />
</BlueLayout>
