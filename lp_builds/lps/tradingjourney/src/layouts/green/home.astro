---
import GreenLayout from "@/layouts/green/pageLayout.astro";
import Section1 from "@/sections/green/home/Section1.astro";
import Section2 from "@/sections/green/home/Section2.astro";
import { type ComponentProps } from "../../../../../globals/types/ComponentProps";

interface Props {
  componentProps: ComponentProps;
}

const { componentProps } = Astro.props;
---

<GreenLayout componentProps={componentProps}>
  <Section1 componentProps={componentProps} />
  <Section2 componentProps={componentProps} />
</GreenLayout>
