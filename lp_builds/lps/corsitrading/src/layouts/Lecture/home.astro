---
import { type ComponentProps } from "../../../../../globals/types/ComponentProps";
import Lecture from "@/layouts/Lecture/pageLayout.astro";
import Section1 from "@/sections/Lecture/home/Section1.astro";
import Section2 from "@/sections/Lecture/home/Section2.astro";
import Section3 from "@/sections/Lecture/home/Section3.astro";

interface Props {
  componentProps: ComponentProps;
}

const { componentProps } = Astro.props;
---

<Lecture componentProps={componentProps}>
  <Section1 componentProps={componentProps} />
  <Section2 componentProps={componentProps} />
  <Section3 componentProps={componentProps} />
</Lecture>
